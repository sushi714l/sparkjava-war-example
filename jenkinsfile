pipeline {
    agent any
     tools{
         maven 'maven'
     }

    stages {
        stage('build') {
            steps {
                sh 'mvn clean package'
                 }
        }
        stage('dockerbuild'){
            steps{
                sh'docker build -t sushil714/project:01 .'
            }
        }
        stage ('dockerpush'){
        steps{
          sh'docker login -u sushil714 -p 8447784955@d'
          sh'docker push sushil714/project:01'
          }
         }
         
    }
}
